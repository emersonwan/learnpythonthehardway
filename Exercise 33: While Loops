# -*- coding: utf-8 -*-
# Rodar como: python lpthw.py
print "\nExercício 33" # Exercício 33
i = 5
numbers = []

while i < 11:
	print "At the top i is %d" % i # O valor de i antes de append
	numbers.append(i)

	i = i + 1
	print "Numbers now: ", numbers # Imprime a lista com o valor de i antes de append
	print "At the bottom i is %d" % i # O valor de i depois de append
	# Repete o while-loop

print "The numbers: "

for num in numbers: # Imprime o conteúdo de numbers
	print num
